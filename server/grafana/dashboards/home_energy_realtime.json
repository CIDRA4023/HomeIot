{
  "title": "Home Energy - Realtime",
  "timezone": "browser",
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Instant Power (W)",
      "datasource": {
        "type": "influxdb",
        "uid": null,
        "name": "InfluxDB"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 10
      },
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: v.defaultBucket)\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"smartmeter_power\" and r[\"_field\"] == \"power_w\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  |> yield(name: \"mean\")",
          "queryType": "flux"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "watt"
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Energy Import (kWh)",
      "datasource": {
        "type": "influxdb",
        "uid": null,
        "name": "InfluxDB"
      },
      "gridPos": {
        "x": 0,
        "y": 10,
        "w": 24,
